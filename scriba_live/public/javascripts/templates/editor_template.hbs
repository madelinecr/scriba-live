<h>Editor</h>
<div id="editor-container">
  <div id="editor-menu">
    <button id="select-tool" {{action "goHome"}}>Home</button>
    <button id="select-tool" {{action "setTool" "select"}}>Select</button>
    <button id="text-tool" {{action "setTool" "text"}}>Text</button>
    <button id="path-tool" {{action "setTool" "path"}}>Path</button>
    <button id="rect-tool" {{action "setTool" "rect"}}>Rect</button>
    <button id="oval-tool" {{action "setTool" "oval"}}>Oval</button>
    <input type='text' id="fill-palette"/>
    <input type='text' id="line-palette"/>
    <select id="line-width">
      <option value="0">0</option>
      <option value="1">1</option>
      <option value="2" selected="selected">2</option>
      <option value="3">3</option>
      <option value="4">4</option>
      <option value="5">5</option>
      <option value="6">6</option>
    </select>
    <button id="add-page" {{action "addPage"}}>New Page</button>
    <button id="prev-page" {{action "prevPage"}}>&lt;</button>
    <button id="next-page" {{action "nextPage"}}>&gt;</button>
    <span id="page-num">Page 1 of 0</span>
  </div>
<div id="editor-canvases"></div>
</div>
<div id="dino-notes">
    <input id="date-picker" type="text"/>
    <ul>
    {{#each note in controller.dino_notes}}
        <li>
        {{note.user_name}}
        <input type="button" value="View Note" {{action "changeNote" note}} />
        </li>
    {{/each}}
    </ul>
</div>